{
  "yaml.schemas": {
    "http://json.schemastore.org/travis": ".travis.yml",
    "https://github.com/pcit-ce/pcit/raw/master/config/config_schema.json": [
      ".pcit.yml",
      ".pcit.yaml"
    ],
    "windows/lnmp-windows-pm-repo/lwpm-json-schema.json": [
      "lwpm.yaml",
      "lwpm.yml"
    ],
    "scripts/mutagen/schema.json" : [
      "mutagen.yml",
      "mutagen.example.yml"
    ]
  },
  "json.schemas": [
    {
      "fileMatch": [
        "lwpm.json"
      ],
      "url": "./windows/lnmp-windows-pm-repo/lwpm-json-schema.json"
    }
  ],
  "yaml.trace.server": "verbose",
  "vs-kubernetes": {
    // "vs-kubernetes.kubectl-path": "C:\\bin\\kubectl.exe",
  },
  "editor.fontFamily": "JetBrains Mono, Consolas, 'Courier New', monospace",
  "editor.fontSize": 20,
  "sync.gist": "76f5fad88768a18910fadf6f8bc35c6c",
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "files.eol": "\n",
