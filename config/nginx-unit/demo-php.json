{
  "listeners": {
    "*:80": {
      "pass": "applications/demo"
    }
  },
  "applications": {
    "demo": {
      "type": "php",
      "processes": 10,
      "root": "/app/demo/public",
      "index": "index.php",
      "user": "root",
      "group": "root"
    }
  },
  "access_log": "/var/log/nginx-unit/access.log"
}
